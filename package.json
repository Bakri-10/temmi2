{
  "name": "fineos-cloud-control-hub",
  "version": "2.0.0-snapshot.0",
  "iacComponent": "FCC_Cloud_Control_Hub",
  "bin": {
    "fineos-cloud-control-hub": "bin/fineos-cloud-control-hub.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --detectOpenHandles --color",
    "cdk": "cdk",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint bin/**/*.ts lib/**/*.ts test/**/*.test.ts",
    "synth": "cdk synth --verbose",
    "deploy": "cdk deploy --app ./cdk.out --verbose",
    "license-report": "npx mkdirp build/reports && echo 'y' | npx license-checker --summary > build/reports/fineos-cloud-control-hub-license-report.txt",
    "vulnerability-report": "npx mkdirp build/reports && npm audit --json | npx npm-audit-html --output build/reports/fineos-cloud-control-hub-vulnerability-report.html"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.122",
    "@types/cfn-response": "1.0.8",
    "@types/jest": "29.5.5",
    "@types/js-yaml": "4.0.6",
    "@types/node": "20.6.3",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "ajv": "8.12.0",
    "aws-cdk": "2.152.0",
    "aws-lambda": "1.0.7",
    "aws-sdk": "2.1650.0",
    "aws-sdk-mock": "5.8.0",
    "cfn-response": "1.0.1",
    "compare-versions": "6.1.0",
    "esbuild": "0.19.3",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-junit": "16.0.0",
    "js-mock": "2.0.0",
    "js-yaml": "4.1.0",
    "prettier": "3.0.3",
    "stream": "0.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "unzipper": "0.10.14"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.649.0",
    "@aws-sdk/client-s3": "3.649.0",
    "@aws-sdk/client-sns": "3.649.0",
    "@fineos-lambda-tools/helpers": "0.7.3-snapshot.4",
    "@fineos-sdk/event-hub-framework": "0.7.3-snapshot.4",
    "@fineos/cdk-constructs": "2.15.3-snapshot.4",
    "@types/js-yaml": "4.0.6",
    "ajv": "8.12.0",
    "aws-cdk-lib": "2.152.0",
    "compare-versions": "6.1.0",
    "constructs": "10.3.0",
    "js-yaml": "4.1.0",
    "source-map-support": "0.5.21",
    "stream": "^0.0.2",
    "unzipper": "0.10.14"
  }
}
